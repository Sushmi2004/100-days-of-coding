// Compare The Triplets
// Platform: Hackerrank

#include<iostream>
using namespace std;
int main()
{
    int a[3];
    int b[3];
    for(int i=0;i<3;i++)
    {
      cin>>a[i];  
     }
    for(int i=0;i<3;i++)
    {
        cin>>b[i];
    }
    int ascore=0;
    int bscore=0;
    for(int i=0;i<3;i++)
    {
    if(a[i]>b[i]){
      ascore++;
    }
    else if(a[i]<b[i]){
        bscore++;
    }
    else{
        ascore+=0;
        bscore+=0;
    }
   }
    cout<<ascore<<" "<<bscore;
    return 0;
}
