// Mahasena
// Platform:Codechef

#include <iostream>
using namespace std;
int main() {
    int N;
    cin>>N;
    int A[N]; 
    int lucky,unlucky=0;
    for(int i=0;i<N;i++)
    {
        cin>>A[i];
        if(A[i]%2==0)
        {
            lucky++;
        }
        else
        {
            unlucky++;    
        }
    }
    
    if(lucky>unlucky)
    {
        cout<<"READY FOR BATTLE"<<endl;
    }
	else
	{
	    cout<<"NOT READY"<<endl;
	}
	return 0;
}
