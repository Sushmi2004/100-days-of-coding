// Zoos
// Platform: Hackerearth

#include <iostream>
using namespace std;
int main()
{
  string s;
  int x=0, y=0;
  cin>>s;
  for(int i=0; i<s.length(); i++)
  {
    if(s[i] == 'z'){
      x++;
      }
    else{
      y++;
      }
  }
  if(2*x ==y)
  {
    cout<<"Yes"<<endl;
  }
  else
  {
   cout<<"No"<<endl;
  }
return 0;
}
  
