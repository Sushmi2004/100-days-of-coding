// Increase IQ 
// Platform: Codechef


#include <iostream>
using namespace std;

int main() {
	int X;
	cin>>X;
	if((X+7)>170)
	{
	    cout<<"yes"<<endl;
	}
	else
	{
	    cout<<"no"<<endl;
	}
	return 0;
}


// Battery Low

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int X;
	    cin>>X;
	    if(X<=15)
	    {
	       cout<<"yes"<<endl;
	    }
	    else
	    {
	       cout<<"no"<<endl; 
	    }
	}
	return 0;
}
